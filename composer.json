{
    "name": "purplship/purplship-php",
    "version": "2021.2",
    "description": "Purplship Multi-carrier Shipping API PHP client library",
    "keywords": [
        "purplship",
        "php",
        "sdk",
        "api",
        "multi-carrier",
        "shipping-api",
        "fedex-api",
        "ups-api",
        "dhl-api"
    ],
    "homepage": "https://github.com/purplship/purplship-php-client",
    "license": "MIT",
    "authors": [{
        "name": "Purplship Team",
        "homepage": "https://purplship.com"
    }],
    "repositories": [{
        "type": "vcs",
        "url": "git@github.com:Purplship/purplship-php-client.git"
    }],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Purplship\\": "lib/" }
    }
}