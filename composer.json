{
    "name": "karrio/karrio-php",
    "version": "2022.3.0",
    "description": "karrio Multi-carrier Shipping API PHP client library",
    "keywords": [
        "karrio",
        "php",
        "sdk",
        "api",
        "multi-carrier",
        "shipping-api",
        "fedex-api",
        "ups-api",
        "dhl-api"
    ],
    "homepage": "https://github.com/karrioapi/karrio-php",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "karrio",
            "homepage": "https://karrio.io"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:karrioapi/karrio-php.git"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Karrio\\": "lib/"
        }
    }
}