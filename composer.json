{
    "name": "purplship/purplship-php",
    "version": "2021.10",
    "description": "purplship Multi-carrier Shipping API PHP client library",
    "keywords": [
        "purplship",
        "php",
        "sdk",
        "api",
        "multi-carrier",
        "shipping-api",
        "fedex-api",
        "ups-api",
        "dhl-api"
    ],
    "homepage": "https://github.com/purplship/purplship-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "purplship",
            "homepage": "https://purplship.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:purplship/purplship-php-client.git"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Purplship\\": "lib/"
        }
    }
}